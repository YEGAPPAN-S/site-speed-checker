{
    "manifest_version": 3,
    "name": "Site Speed Checker",
    "description": "Check the performance of 10 websites using PageSpeed Insights API.",
    "version": "1.0",
    "permissions": ["storage", "activeTab"],
    
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://www.googleapis.com/*"
    ]
  }
  